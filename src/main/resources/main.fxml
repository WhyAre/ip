<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<VBox xmlns:fx="http://javafx.com/fxml/1" prefHeight="400.0" prefWidth="600.0" spacing="5.0"
      xmlns="http://javafx.com/javafx/17.0.12" fx:controller="jank.ui.MainController">
    <children>
        <TextArea fx:id="outputbox" editable="false" focusTraversable="false" prefHeight="353.0" prefWidth="590.0"
                  wrapText="true" VBox.vgrow="ALWAYS">
            <font>
                <Font name="Monospaced Regular" size="13.0"/>
            </font>
        </TextArea>
        <TextField fx:id="inputbox" onKeyPressed="#inputOnKeyPressed" prefWidth="600.0"/>
    </children>
    <padding>
        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
    </padding>
</VBox>
